{
  "name": "gisti",
  "version": "0.0.12",
  "description": "GISTI - The interactive CLI for gist",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node index.js",
    "lint": "eslint main.js ./lib/**/*.js",
    "prepublish": "npm run man",
    "man": "cd man && marked-man --version 'v1.0.0' --manual 'GISTI Help' --section 1 man.md > gisti.1"
  },
  "repository": {
    "type": "gist",
    "url": "git+https://github.com/pascalrogalla/gisti.git"
  },
  "keywords": [
    "gist",
    "cli",
    "github",
    "nodejs",
    "js",
    "es"
  ],
  "author": "@pascalrogalla",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pascalrogalla/gisti/issues"
  },
  "homepage": "https://github.com/pascalrogalla/gisti#readme",
  "dependencies": {
    "@octokit/rest": "^16.35.0",
    "chalk": "^3.0.0",
    "clear": "^0.1.0",
    "clipboardy": "^2.1.0",
    "clui": "^0.3.6",
    "commander": "^4.0.1",
    "configstore": "^5.0.0",
    "esm": "^3.2.25",
    "figlet": "^1.2.4",
    "inquirer": "^7.0.0",
    "lolcatjs": "^2.3.2",
    "minimist": "^1.2.0",
    "opn": "^6.0.0"
  },
  "bin": {
    "gisti": "./index.js"
  },
  "directories": {
    "lib": "lib"
  },
  "devDependencies": {
    "eslint": "^6.7.2",
    "marked": "^0.7.0",
    "marked-man": "^0.7.0"
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "es5",
    "singleQuote": true,
    "semi": false,
    "jsxBracketSameLine": false,
    "bracketSpacing": true
  }
}
